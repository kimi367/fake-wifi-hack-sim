import random
import pyperclip
import time

# æ¨¡æ‹Ÿä¸€ä¸ªæ­£ç¡®WiFiå¯†ç ï¼ˆæ¯”å¦‚ä½ å¦ˆè®¾ç½®çš„ï¼‰ï¼ˆè¿™é‡Œä¸ºäº†æé«˜é€Ÿåº¦ï¼Œæœ¬äººè®¾ç½®äº†1ä½æ•°ï¼Œè‡ªå·±å¯ä»¥æ”¹ï¼‰
real_pass = "9"

# æ¨¡æ‹Ÿçˆ†ç ´
attempts = 0
while True:
    fake_pass = ''.join(random.choices('0123456789', k=1))
    attempts += 1
    print(f'ğŸš€ å°è¯•ç¬¬ {attempts} æ¬¡ï¼Œå£ä»¤ï¼š{fake_pass}')

    # æ¨¡æ‹Ÿâ€œçŒœä¸­â€
    if fake_pass == real_pass:
        print(f'âœ… WiFiå ä¾¿å®œæˆåŠŸ!!! å¯†ç æ˜¯ï¼š{fake_pass}')
        pyperclip.copy(fake_pass)
        break
    else:
        print(f'ğŸ˜¤ è€å­Pythonä¸æƒ³åŠ¨äº†ï¼Œä½†è¿˜æ˜¯ç»§ç»­â€¦â€¦ [{fake_pass}]')
        time.sleep(0.1)  # ä¸ºäº†çœ‹å¾—æ¸…ï¼Œå»¶è¿Ÿä¸€ä¸‹
